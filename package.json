{
  "name": "react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:react": "tsc && vite build",
    "build": "turbo run build:react",
    "preview": "vite preview",
    "prepare": "husky install",
    "test:react": "vitest run",
    "test": "turbo run test:react",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,mjs,cjs}\"",
    "lint:react": "eslint \"**/*.{ts,tsx,js,jsx,mjs,cjs}\"",
    "lint": "turbo run lint:react",
    "lint:fix": "eslint --fix \"**/*.{ts,tsx,js,jsx,mjs,cjs}\"",
    "type:check:react": "tsc --noEmit",
    "type:check": "turbo run type:check:react",
    "format:check:react": "prettier --check \"**/*.{ts,tsx,js,jsx,mjs,cjs}\"",
    "format:check": "turbo run format:check:react",
    "ci-test": "turbo run lint:react type:check:react format:check:react --parallel"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.17.10",
    "@tanstack/react-router": "^1.4.6",
    "clsx": "^2.1.0",
    "ky": "^1.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.17.12",
    "@tanstack/router-cli": "^1.4.6",
    "@tanstack/router-devtools": "^1.4.6",
    "@tanstack/router-vite-plugin": "^1.4.6",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.11.1",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/ui": "^1.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "happy-dom": "^13.1.4",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "msw": "^2.0.14",
    "postcss": "^8.4.33",
    "prettier": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "shell-quote": "^1.8.1",
    "tailwindcss": "^3.4.1",
    "turbo": "^1.11.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.2.0",
    "vitest-dom": "^0.1.1"
  }
}
