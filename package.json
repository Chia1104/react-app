{
  "name": "react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:react": "tsc && vite build",
    "build": "turbo run build:react",
    "preview": "vite preview",
    "prepare": "husky",
    "test:react": "vitest run",
    "test": "turbo run test:react",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,mjs,cjs}\"",
    "lint:react": "eslint \"**/*.{ts,tsx,js,jsx,mjs,cjs}\"",
    "lint": "turbo run lint:react",
    "lint:fix": "eslint --fix \"**/*.{ts,tsx,js,jsx,mjs,cjs}\"",
    "type:check:react": "tsc --noEmit",
    "type:check": "turbo run type:check:react",
    "format:check:react": "prettier --check \"**/*.{ts,tsx,js,jsx,mjs,cjs}\"",
    "format:check": "turbo run format:check:react",
    "ci-test": "turbo run lint:react type:check:react format:check:react --parallel"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.28.8",
    "@tanstack/react-router": "^1.22.7",
    "clsx": "^2.1.0",
    "ky": "^1.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.2",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.28.8",
    "@tanstack/router-cli": "^1.22.6",
    "@tanstack/router-devtools": "^1.22.7",
    "@tanstack/router-vite-plugin": "^1.22.6",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.71",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@vitest/ui": "^1.4.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "happy-dom": "^14.3.8",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "msw": "^2.2.11",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "shell-quote": "^1.8.1",
    "tailwindcss": "^3.4.1",
    "turbo": "^1.13.0",
    "typescript": "^5.4.3",
    "vite": "^5.2.6",
    "vitest": "^1.4.0",
    "vitest-dom": "^0.1.1"
  }
}
